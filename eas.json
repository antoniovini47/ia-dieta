{
  "cli": {
    "version": ">= 10.2.2"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": "true"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "distribution": "store",
      "env": {
        "EXPO_PUBLIC_GEMINI_API_KEY": "$EXPO_PUBLIC_GEMINI_API_KEY$",
        "EXPO_PUBLIC_APP_AD_ID": "$EXPO_PUBLIC_APP_AD_ID$",
        "EXPO_PUBLIC_BANNER_AD_UNIT_ID": "$EXPO_PUBLIC_BANNER_AD_UNIT_ID$",
        "EXPO_PUBLIC_INTERSTITIAL_AD_UNIT_ID": "$EXPO_PUBLIC_INTERSTITIAL_AD_UNIT_ID$",
        "EXPO_PUBLIC_IS_USER_PREMIUM":"true"
      },
      "android": {
        "buildType": "app-bundle",
        "autoIncrement": "versionCode"
      }
    },
    "testProduction": {
      "distribution": "store",
      "env": {
        "EXPO_PUBLIC_GEMINI_API_KEY": "$EXPO_PUBLIC_GEMINI_API_KEY$",
        "EXPO_PUBLIC_APP_AD_ID": "$EXPO_PUBLIC_APP_AD_ID$",
        "EXPO_PUBLIC_BANNER_AD_UNIT_ID": "$EXPO_PUBLIC_BANNER_AD_UNIT_ID$",
        "EXPO_PUBLIC_INTERSTITIAL_AD_UNIT_ID": "$EXPO_PUBLIC_INTERSTITIAL_AD_UNIT_ID$",
        "EXPO_PUBLIC_IS_USER_PREMIUM":"true"
      },
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
